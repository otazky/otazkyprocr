<section id="elections-detail">
  <h2><%= @election.election_type.name + ' ' + @election.held.strftime('%Y') %></h2>
  <div><%= raw @election.description %></div>


  <% if [4,5].include? @election.election_type_id %>
      <%= render 'citizens' %>
  <% else %>
    <%= render 'politicians' %>

  <% end %>



</section>
